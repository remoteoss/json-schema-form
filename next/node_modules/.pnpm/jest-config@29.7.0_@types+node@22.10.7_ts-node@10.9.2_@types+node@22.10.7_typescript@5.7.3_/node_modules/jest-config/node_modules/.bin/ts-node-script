#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/ts-node@10.9.2_@types+node@22.10.7_typescript@5.7.3/node_modules/ts-node/dist/node_modules:/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/ts-node@10.9.2_@types+node@22.10.7_typescript@5.7.3/node_modules/ts-node/node_modules:/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/ts-node@10.9.2_@types+node@22.10.7_typescript@5.7.3/node_modules:/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/ts-node@10.9.2_@types+node@22.10.7_typescript@5.7.3/node_modules/ts-node/dist/node_modules:/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/ts-node@10.9.2_@types+node@22.10.7_typescript@5.7.3/node_modules/ts-node/node_modules:/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/ts-node@10.9.2_@types+node@22.10.7_typescript@5.7.3/node_modules:/home/simone/Desktop/json-schema-form/next/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ts-node@10.9.2_@types+node@22.10.7_typescript@5.7.3/node_modules/ts-node/dist/bin-script.js" "$@"
else
  exec node  "$basedir/../../../../../ts-node@10.9.2_@types+node@22.10.7_typescript@5.7.3/node_modules/ts-node/dist/bin-script.js" "$@"
fi
