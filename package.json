    "@jest/reporters": "^29.7.0",
    "@types/json-logic-js": "^2.0.8",
    "@types/lodash": "^4.17.16",
    "@types/validator": "^13.12.2",
    "babel-jest": "^29.7.0",
    "eslint": "^9.18.0",
    "generate-changelog": "^1.8.0",
    "jest": "^29.7.0",
    "json-schema-typed": "^8.0.1",
    "lodash": "^4.17.21",
    "randexp": "^0.5.3",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  }
